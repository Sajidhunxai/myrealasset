
# DO NOT REMOVE THIS LINE AND THE LINES BELLOW SSL_REDIRECT:myrealasset.net
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} (www\.)?myrealasset.net
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
